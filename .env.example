# Local Development Ports (customize these if you have conflicts)
FRONTEND_PORT=3000
BACKEND_PORT=8000
REDIRECT_PORT=8001
DYNAMODB_PORT=8002
DYNAMODB_ADMIN_PORT=8003

# Local Development URLs
NODE_ENV=development
VITE_API_URL=http://localhost:8000
VITE_REDIRECT_URL=http://localhost:8001

# AWS Production
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=your-account-id
DYNAMODB_TABLE_NAME=linkpipe-urls
API_GATEWAY_URL=https://your-api-id.execute-api.us-east-1.amazonaws.com

# Vercel Frontend (if using Vercel)
VERCEL_PROJECT_ID=your-vercel-project-id
VERCEL_ORG_ID=your-vercel-org-id

# Custom Domains
PRIMARY_DOMAIN=yourdomain.com
REDIRECT_DOMAINS=short.yourdomain.com,go.yourdomain.com

# Optional: Basic Auth for Admin
ADMIN_USERNAME=admin
ADMIN_PASSWORD=changeme123

# UTM Parameter Defaults
DEFAULT_UTM_SOURCE=linkpipe
DEFAULT_UTM_MEDIUM=shortlink 